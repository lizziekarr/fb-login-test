{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "indent": ["error", 4],
        "semi": "error",
        "eqeqeq": "error",
        "curly": "error",
        "quotes": ["error", "single"]
    }
}
